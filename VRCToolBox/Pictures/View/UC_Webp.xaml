<UserControl x:Class="VRCToolBox.Pictures.View.UC_Webp"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:pvm="clr-namespace:VRCToolBox.Pictures.ViewModel"
             xmlns:ui="http://schemas.modernwpf.com/2019"
             xmlns:local="clr-namespace:VRCToolBox.Pictures.View"
             mc:Ignorable="d" d:DataContext="{d:DesignInstance IsDesignTimeCreatable=True, Type=pvm:WebpEncoderOptionsViewModel}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="66" VerticalAlignment="Top" HorizontalAlignment="Left" Width="390" Margin="2,24,0,0">
            <TextBlock Text="変換時の品質" FontSize="24" Height="34" Width="148"/>
            <ui:NumberBox Value="{Binding Quality.Value}" Maximum="100" Minimum="0" SpinButtonPlacementMode="Inline"
                      FontSize="24" Height="46" Width="237"/>
        </StackPanel>
    </Grid>
</UserControl>
